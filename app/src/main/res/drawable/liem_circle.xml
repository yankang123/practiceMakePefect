<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="50dp" android:height="50dp">
        <shape>
            <solid android:width="2dp" android:color="@color/black"/>

            <corners android:radius="0dp"/>
        </shape>
    </item>
    <item android:top="10dp" android:bottom="1dp"
        android:left="1dp" android:right="1dp">
        <shape>
            <solid android:color="@color/white"/>
            <corners android:radius="0dp"/>
        </shape>
    </item>
    <item android:top="20dp" android:bottom="1dp"
        android:left="1dp" android:right="1dp">
        <shape>
            <solid android:color="@color/purple_200"/>
            <corners android:radius="0dp"/>
        </shape>
    </item>
</layer-list>